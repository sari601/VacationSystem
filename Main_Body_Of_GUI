    public class ButtonHandler implements ActionListener {
        
        public void actionPerformed(ActionEvent actionEvent) {
            
              if (actionEvent.getSource() == button1 )
              {

                     x = 0;
                     frame1.setVisible(true);
                     frame0.getRootPane().getParent().setVisible(false);
                     
                     
              }
              else if (actionEvent.getSource() == farrightChild)
              {

                     x = x + 1000;
                     frame2.setVisible(true);
                     frame1.setVisible(false);
                   
                     
              }
              else if (actionEvent.getSource() == farleftChild)
              {
                     x = x + 2000;
                     frame2.setVisible(true);
                     frame1.setVisible(false);
                    
              }
              else if (actionEvent.getSource() == middleChild)
              {
                     x = x + 3000;
                     frame2.setVisible(true);
                     frame1.setVisible(false);
                   
              }
              else if (actionEvent.getSource() == rightChild)
              {
                    x = x + 5000;
                     frame2.setVisible(true);
                     frame1.setVisible(false);
                    
              }
              else if (actionEvent.getSource() == leftChild)
              {
                     x = x + 4000;
                     frame2.setVisible(true);
                     frame1.setVisible(false);
              }
              else if (actionEvent.getSource() == FirstRight) // belew
              {
                     y = 0;
                     x = x + 200;
                     frame3.setVisible(true);
                     frame2.setVisible(false);
                  
              }
              else if (actionEvent.getSource() == FirstLeft) // above
              {
                     y = 1;
                     x = x + 500;
                     frame3.setVisible(true);
                     frame2.setVisible(false);
                   
              }
              else if (actionEvent.getSource() == SecondRight) // coast
              {
                     x = x + 10;
                     frame4.setVisible(true);
                     frame3.setVisible(false);
                   
              }
              else if (actionEvent.getSource() == SecondLeft) // no coast
              {
                     x = x + 20;
                     frame4.setVisible(true);
                     frame3.setVisible(false);
                   
              }
              else if (actionEvent.getSource() == ThirdRight) // Single
              {
                     x = x + 2;
                     frame5.setVisible(true);
                     frame4.setVisible(false);
              }
              else if (actionEvent.getSource() == ThirdLeft) // family
              {
                     x = x + 1;
                     frame5.setVisible(true);
                     frame4.setVisible(false);
              }
              else if (actionEvent.getSource() == ForthRight) // country
              {
                     x = x + 30;
                     frame6.setVisible(true);
                     frame5.setVisible(false);
              }
              else if (actionEvent.getSource() == ForthLeft) // crowded
              {
                     x = x + 60;
                     frame6.setVisible(true);
                     frame5.setVisible(false);
              }
              else if (actionEvent.getSource() == FifthRight) // yes
              {
                     x = x + 1;
                     frame7.setVisible(true);
                     frame6.setVisible(false);
              }
              else if (actionEvent.getSource() == FifthLeft) // no
              {
                     x = x + 2;
                     frame7.setVisible(true);
                     frame6.setVisible(false);
              }
              else if (actionEvent.getSource() == SixthRight) // warm

              {
                     x = x + 50;
                     frame8.setVisible(true);
                     frame7.setVisible(false);
              }
              else if (actionEvent.getSource() == SixthLeft) // cold
              {
                    x = x + 100;
                     frame8.setVisible(true);
                     frame7.setVisible(false);
              }
              else if (actionEvent.getSource() == SeventhRight) // night
              {
                    x = x + 5;
                     frame9.setVisible(true);
                     frame8.setVisible(false);
              }
              else if (actionEvent.getSource() == SeventhLeft) // day
              {
                     x = x + 10;
                     frame9.setVisible(true);
                     frame8.setVisible(false);
              }
              else if (actionEvent.getSource() == EightRight) // less then a week
              {
                  if(y == 0)
                  {
                    x = x + 200;
                     frame9.setVisible(false);
                     frame10.setVisible(true);
                     //JOptionPane.showMessageDialog(null, x);
                  }
                  else
                     {
                     x = x + 300;
                     frame9.setVisible(false);
                     frame10.setVisible(true);
                     //JOptionPane.showMessageDialog(null, x);
                     }
              }
              else if (actionEvent.getSource() == EightLeft) // more then a week
              {
                  if(y == 1)
                     {
                    x = x + 300;
                     frame9.setVisible(false);
                     frame10.setVisible(true);
                     //JOptionPane.showMessageDialog(null, x);
                     }
                  else
                     {
                     x = x + 200;
                     frame9.setVisible(false);
                     frame10.setVisible(true);
                     //JOptionPane.showMessageDialog(null, x);
                     }
                    
              }
              else if (actionEvent.getSource() == NinthRight) // yes
              {
                     x = x + 2;
                     frame11.setVisible(true);
                     frame10.setVisible(false);
                  //   Conclusion.setText(" " + x);

              }
              else if (actionEvent.getSource() == NinthLeft) // no
              {
                     x = x + 1;
                     frame11.setVisible(true);
                     frame10.setVisible(false);
                   //  Conclusion.setText(" "+ x);
                      
              }
              else if (actionEvent.getSource() == Exit){
                    System.exit(0);
                     //JOptionPane.showMessageDialog(null, x);
                     //frame0.getRootPane().getParent().setEnabled(false);
              }
              else if (actionEvent.getSource() == Result)

                  FinalStep();

        }
    }

            public void FinalStep(){
                  // outside Asia
                  if (  x == 1986 || x == 1985 || x == 1980 || x == 1981 || x == 1984 || x == 1983 || x == 1982 || x == 1987 || x == 1988 || x == 1989)// cold + crowded + coast
                  {
                  JOptionPane.showMessageDialog(null, "Country : Australia, City : Melbourne");
                  }
                 else if ( x == 1946 || x == 1945 || x == 1940 || x == 1941 || x == 1944 || x == 1943 || x == 1942 || x == 1947 || x == 1948 || x == 1949)// warm + crowded + coast
                  {
                  JOptionPane.showMessageDialog(null, "Country : Egypt, City : Sharm Al Shaik");
                  }
                 else if ( x == 1966 || x == 1965 || x == 1960 || x == 1961 || x == 1964 || x == 1963 || x == 1962 || x == 1967 || x == 1968 || x == 1969)// cold + country + no coast
                  {
                  JOptionPane.showMessageDialog(null, "Country : Slovakia, City : Martin");
                  }
                 else if ( x == 1916 || x == 1915 || x == 1910 || x == 1911 || x == 1914 || x == 1913 || x == 1912 || x == 1917 || x == 1918 || x == 1919)//warm + country + no coast
                  {
                  JOptionPane.showMessageDialog(null, "Country :South Africa, City : Mahikeng");
                  }
                 else if (x == 1906 || x == 1905 || x == 1900 || x == 1901 || x == 1904 || x == 1903 || x == 1902 || x == 1907 || x == 1908 || x == 1909)// warm + country + coast
                  {
                  JOptionPane.showMessageDialog(null, "Country : Australia, City : Albany");
                  }
                  else if (x == 1956 || x == 1955 || x == 1950 || x == 1951 || x == 1954 || x == 1953 || x == 1952 || x == 1957 || x == 1958 || x == 1959)// cold + country + coast
                  {
                  JOptionPane.showMessageDialog(null, "Country : Sweeden, City : Stockholm");
                  }
